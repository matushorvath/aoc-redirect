    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/main.js",
            "env": {
                "AWS_SDK_LOAD_CONFIG": "1",
                "AWS_PROFILE": "private",
                "DEBUG": "express:*"
            }
        }
    ]
}
